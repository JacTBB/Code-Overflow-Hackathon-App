{
  "expo": {
    "name": "Code Overflow App",
    "slug": "code-overflow-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#b0e9be"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpeg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "fitquest"
  }
}
